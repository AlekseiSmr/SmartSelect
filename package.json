{
    "name": "smart-selection",
    "displayName": "SmartSelection",
    "description": "This extension has varios ability to select multi lines.",
    "version": "1.0.0",
    "publisher": "alekseiermolov",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.enterFormula"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.enterFormula",
                "title": "Smart select: Enter formula"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "icon": "images/logo.png",
    "license": "MIT",
    "keywords": [
        "select lines",
        "smart select",
        "formula",
        "multi select"
    ],
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "@types/mathjs": "^4.4.1",
        "mathjs": "^5.1.1"
    }
}
